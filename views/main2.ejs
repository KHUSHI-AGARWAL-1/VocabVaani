<h1>
  word:<%= word%>
 </h1> 
<p>Definition: <%= dictionaryResult.definition%></p>


<h2>Antonyms</h2>
<ul>
  <% thesaurusResult.antonyms.slice(0, 6).forEach((antonym) => { %>
    <li><%= antonym %></li>
  <% }); %>
</ul>
<h2> Synonyms </h2>
<ul>
  <% thesaurusResult.synonyms.slice(0, 6).forEach((antonym) => { %>
    <li><%= antonym %></li>
  <% }); %>
</ul>
<h2>Rhyming Words</h2>
<!-- Display the rhyming words -->
<h2>Rhyming Words</h2>
<ul>
  <% rhymeResult.slice(0,6).forEach((rhyme) => { %>
    <li><%= rhyme %></li>
  <% }); %>
</ul>
